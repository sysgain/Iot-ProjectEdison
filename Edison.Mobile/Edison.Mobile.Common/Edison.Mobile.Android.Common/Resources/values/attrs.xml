<?xml version="1.0" encoding="utf-8" ?>
<resources>

  
<!-- Attribute Declarations -->
  
  <!-- CircularImageButton Attributes -->
  <attr name="buttonBackground" format="reference|color" />
  <attr name="buttonBackgroundTint" format="reference|color" />
  <attr name="buttonBackgroundTintMode">
    <!-- The tint is drawn on top of the drawable. [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] -->
    <enum name="src_over" value="3" />
    <!-- The tint is masked by the alpha channel of the drawable. The drawable’s color channels are thrown out. [Sa * Da, Sc * Da] -->
    <enum name="src_in" value="5" />
    <!-- The tint is drawn above the drawable, but with the drawable’s alpha channel masking the result. [Da, Sc * Da + (1 - Sa) * Dc] -->
    <enum name="src_atop" value="9" />
    <!-- Multiplies the color and alpha channels of the drawable with those of the tint. [Sa * Da, Sc * Dc] -->
    <enum name="multiply" value="14" />
    <!-- [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] -->
    <enum name="screen" value="15" />
    <!-- Combines the tint and drawable color and alpha channels, clamping the result to valid color values. Saturate(S + D) -->
    <enum name="add" value="16" />
  </attr>
  <attr name="iconSrc" format="reference" />
  <attr name="buttonRippleColor" format="reference|color" />
  <attr name="iconPaddingLeft" format="dimension" />
  <attr name="iconPaddingTop" format="dimension" />
  <attr name="iconPaddingRight" format="dimension" />
  <attr name="iconPaddingBottom" format="dimension" />
  <attr name="iconPadding" format="dimension" />
  <attr name="selected" format="boolean" />
  
  <!-- CircularProfileView Attributes -->
  <attr name="borderWidth" format="dimension" />
  <attr name="borderColor" format="reference|color" />
  <attr name="textPadding" format="dimension" />
  <attr name="controlRippleColor" format="reference|color" />
  
  <!-- CircularEventGauge Attributes -->
  <attr name="ringStartColor" format="reference|color" />
  <attr name="ringEndColor" format="reference|color" />
  <attr name="centerTint" format="reference|color" />
  <attr name="centerSrc" format="reference" />
  <attr name="ringGap" format="dimension" />
  <attr name="countTypeface">
    <enum name="normal" value="0" />
    <enum name="sans" value="1" />
    <enum name="serif" value="2" />
    <enum name="monospace" value="3" />
  </attr>
  <attr name="countTextStyle">
    <flag name="normal" value="0" />
    <flag name="bold" value="1" />
    <flag name="italic" value="2" />
  </attr>
  <attr name="countFontFamily" format="string" />
  <attr name="countColor" format="reference|color" />
  <attr name="countAutosize" format="boolean" />
  <attr name="countTextSize" format="dimension" />
  <attr name="countAllCaps" format="boolean" />
  <attr name="countText" format="string" />
  <attr name="labelTypeface">
    <enum name="normal" value="0" />
    <enum name="sans" value="1" />
    <enum name="serif" value="2" />
    <enum name="monospace" value="3" />
  </attr>
  <attr name="labelTextStyle">
    <flag name="normal" value="0" />
    <flag name="bold" value="1" />
    <flag name="italic" value="2" />
  </attr>
  <attr name="labelFontFamily" format="string" />
  <attr name="labelColor" format="reference|color" />
  <attr name="labelTextSize" format="dimension" />
  <attr name="labelAllCaps" format="boolean" />
  <attr name="labelText" format="string" />
  <attr name="iconSize" format="dimension" />
  <attr name="count" format="integer" />
  <attr name="active" format="boolean" />

  <!-- RightAngleTriangle Attributes -->
  <!-- <attr name="borderWidth" format="dimension" /> -->  <!-- Already defined -->
  <!-- <attr name="borderColor" format="color" /> -->  <!-- Already defined -->
  <attr name="fillColor" format="reference|color" />
  <attr name="rightAnglePosition">
    <flag name="bottomLeft" value="0" />
    <flag name="topLeft" value="1" />
    <flag name="topRight" value="2" />
    <flag name="bottomRight" value="3" />
  </attr>





<!-- Styleable Declarations -->
  
  <!-- CircularImageButton Styleable -->
  <declare-styleable name="CircularImageButton">
    <attr name="buttonBackground" />
    <attr name="buttonBackgroundTint" />
    <attr name="buttonBackgroundTintMode" />
    <attr name="iconSrc" />
    <attr name="iconTint" />
    <attr name="iconTintMode" />
    <attr name="buttonRippleColor" />
    <attr name="iconPaddingLeft" />
    <attr name="iconPaddingTop" />
    <attr name="iconPaddingRight" />
    <attr name="iconPaddingBottom" />
    <attr name="iconPadding" />
    <attr name="android:tag" />
    <attr name="selected" />
    <attr name="android:enabled" />
  </declare-styleable>

  <!-- CircularProfileView Styleable -->
  <declare-styleable name="CircularProfileView">
    <attr name="android:background" />
    <attr name="android:backgroundTint" />
    <attr name="android:backgroundTintMode" />
    <attr name="android:src" />
    <attr name="borderWidth" />
    <attr name="borderColor" />
    <attr name="android:fontFamily" />
    <attr name="android:textAllCaps" />
    <attr name="android:textColor" />
    <attr name="android:textStyle" />
    <attr name="android:text" />
    <attr name="android:typeface" />
    <attr name="textPadding" />
    <attr name="controlRippleColor" />
  </declare-styleable>


  <!-- CircularEventGauge Styleable -->
  <declare-styleable name="CircularEventGauge">
    <attr name="ringStartColor" />
    <attr name="ringEndColor" />
    <attr name="centerTint" />
    <attr name="centerSrc" />
    <attr name="ringGap" />
    <attr name="countTypeface" />
    <attr name="countTextStyle" />
    <attr name="countFontFamily" />
    <attr name="countColor" />
    <attr name="countAutosize" />
    <attr name="countTextSize" />
    <attr name="countAllCaps" />
    <attr name="countText" />
    <attr name="labelTypeface" />
    <attr name="labelTextStyle" />
    <attr name="labelFontFamily" />
    <attr name="labelColor" />
    <attr name="labelTextSize" />
    <attr name="labelAllCaps" />
    <attr name="labelText" />
    <attr name="iconSrc" />
    <attr name="iconSize" />
    <attr name="iconTint" />
    <attr name="iconTintMode" />
    <attr name="count" />
    <attr name="active" />
    <attr name="controlRippleColor" />
  </declare-styleable>

  <!-- RightAngleTriangle Styleable -->
  <declare-styleable name="RightAngleTriangle">
    <attr name="fillColor" />
    <attr name="borderWidth" />
    <attr name="borderColor" />
    <attr name="rightAnglePosition" />
  </declare-styleable>


</resources>